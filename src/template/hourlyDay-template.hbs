<div class="gallery-visually">
  {{!-- <div class="hourly-weather-wrapper">
    <button class="hourly-weather-close">&#10006;</button>
  </div> --}}
  <ul class="hourly-weather-list" data-slick='{"slidesToShow": 2, "slidesToScroll": 1}'>
    {{#each forecast}}
    <li class="hourly-weather-item" id="Clear">
      <p class="hourly-weather-item__set-time">{{time}}</p>
      <img class="hourly-weather-item__set-img" src="http://openweathermap.org/img/w/{{icon}}.png" alt="icon" />
      <p class="hourly-weather-item__set-temperature">{{main.temp}}&#176;</p>
      <div class="hourly-weather-item__weather-details">
        <div class="hourly-weather-item__wind-value-wrapper">
          <svg class="hourly-weather-item__svg" xmlns="http://www.w3.org/2000/svg"
            xmlns:xlink="http://www.w3.org/1999/xlink" width="22" height="20" viewBox="0 0 22 20">
            <image id="Слой_43" data-name="Слой 43" class="cls-1" width="22" height="20"
              xlink:href="data:img/png;base64," />
          </svg>
          <p class="hourly-weather-item__pressure-value">{{main.pressure}}mm</p>
        </div>
        <div class="hourly-weather-item__wind-value-wrapper">
          <svg class="hourly-weather-item__svgs" xmlns="http://www.w3.org/2000/svg"
            xmlns:xlink="http://www.w3.org/1999/xlink" width="13" height="20" viewBox="0 0 13 20">
            <image id="Слой_44" data-name="Слой 44" class="cls-1" width="13" height="20"
              xlink:href="data:img/png;base64," />
          </svg>
          <p class="hourly-weather-item__humidity-value">{{main.humidity}}%</p>
        </div>
        <div class="hourly-weather-item__wind-value-wrapper">
          <svg class="hourly-weather-item__svg" xmlns="http://www.w3.org/2000/svg"
            xmlns:xlink="http://www.w3.org/1999/xlink" width="24" height="20" viewBox="0 0 24 20">
            <image id="Слой_45" data-name="Слой 45" class="cls-1" width="24" height="20"
              xlink:href="data:img/png;base64," />
          </svg>
          <p class="hourly-weather-item__wind-value">{{wind.speed}}m/s</p>
        </div>
      </div>
    </li>
    {{/each}}
  </ul>
</div>
<div class="hourly-weather-btns">
  <button class="hourly-weather-btns__prev slick-prev"></button>
  <button class="hourly-weather-btns__next slick-next"></button>
</div>
{{!-- вставила "progressbar"  --}}
 <div class="progressbar" role="progressbar" aria-valuemin="25" aria-valuemax="100">
    <span class="progressbar__label sr-only">
  </div>